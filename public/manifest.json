{
    "manifest_version": 2,
    "name": "Jira Plugin",
    "description": "Do more with Jira",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "https://jiradbg.deutsche-boerse.de",
        "https://github.deutsche-boerse.de"
    ]
}